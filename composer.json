    "keywords": ["microframework"],
    "homepage": "https://github.com/Formula9/Core",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
